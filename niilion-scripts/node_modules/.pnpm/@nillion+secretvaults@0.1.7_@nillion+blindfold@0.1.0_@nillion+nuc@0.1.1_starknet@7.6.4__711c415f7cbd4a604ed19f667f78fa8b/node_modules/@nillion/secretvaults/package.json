{
  "name": "@nillion/secretvaults",
  "version": "0.1.7",
  "license": "MIT",
  "description": "A client for interacting with nillion's nildb blind module",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/lib.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "es-toolkit": "^1.39.8",
    "zod": "^4.0.14"
  },
  "peerDependencies": {
    "@nillion/blindfold": "0.1.0",
    "@nillion/nuc": "0.1.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@biomejs/biome": "2.1.3",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@faker-js/faker": "^9.9.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^24.1.0",
    "@types/semver": "^7.7.0",
    "@vitest/coverage-v8": "3.2.4",
    "docker-compose": "^1.2.0",
    "dotenv": "^17.2.1",
    "lefthook": "^1.12.2",
    "mongodb": "^6.18.0",
    "semver": "^7.7.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "type-fest": "^4.41.0",
    "typedoc": "^0.28.10",
    "typescript": "^5.9.2",
    "vite": "^7.0.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "install-hooks": "lefthook install",
    "attw": "attw --profile esm-only --pack .",
    "check": "biome check --fix && tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint --fix",
    "format": "biome format --fix",
    "build": "tsup",
    "test": "vitest run",
    "docs": "typedoc",
    "docs:serve": "pnpm docs && npx serve docs"
  }
}